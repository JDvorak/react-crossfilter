    "@babel/preset-env": "^7.5.5",
    "@babel/preset-react": "^7.0.0",
    "@babel/register": "7.0.0",
    "dependency-check": "^4.1.0",
    "enzyme": "3.7.0",
    "enzyme-adapter-react-16": "1.14.0",
    "istanbul": "^0.4.5",
    "jsdom": "^15.1.1",
    "jsdom-global": "^3.0.2",
    "standard": "^12.0.1",
    "tape": "^4.10.2"
