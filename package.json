{
  "name": "react-crossfilter",
  "version": "1.0.0",
  "description": "React Crossfilter State Providers",
  "main": "index.js",
  "scripts": {
    "deps": "dependency-check . && dependency-check . --extra --no-dev",
    "test": "standard && npm run deps && NODE_ENV=test node -r @babel/register -r jsdom-global/register  test.js",
